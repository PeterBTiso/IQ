            <table class="table table-hover mb-5">
                <thead class="thBack bg-secondary text-light">
                    <h2 class="text-center">Grade 1</h2>
                    <tr>
                        <th>Name</th>
                        <th>Last Name</th>
                        <th>Absences</th>
                        <th>Avg</th>
                        <th>Eng Avg</th>
                        <th>Khm Avg</th>
                    </tr>
                </thead>
                <% for(let student of students) { %>
                    <% if(student.level===1){ %>
                        <tbody>
                            <tr>
                                <td>
                                    <div class="row_data" edit_type="click" col_name="fname"><a
                                            href="/students/<%= student._id %>">
                                            <%= student.firstN %>
                                        </a>
                                    </div>
                                </td>
                                <td>
                                    <div class="row_data" edit_type="click" col_name="lname">
                                        <%= student.lastN %>
                                    </div>
                                </td>
                                <td>
                                    <div class="row_data" edit_type="click" col_name="absc">3</div>
                                </td>
                                <td>
