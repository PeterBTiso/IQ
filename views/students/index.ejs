                <% for(let student of students) { %>
                    <% let totalScore=0 %>
                        <% let studentTotalScore=0 %>
                            <% let studentPercentage=0 %>
                                <div class="row">
                                    <h3>
                                        <!-- <span>
                                <%= student.firstN %>
                                <%= student.lastN %>
                                <%= student.level %>
                            </span> -->
