        <div id="gr1">
            <table class="table table-hover mb-5">
                <thead class="thBack bg-secondary text-light">
                    <h2 class="text-center">Grade 1</h2>
                    <tr>
                        <th>Full Name</th>
                        <th>English Avg</th>
                        <th>Math Avg</th>
                        <th>Science Avg</th>
                        <th>Social S Avg</th>
                    </tr>
                </thead>
                <% for(let student of students) { %>
                    <% if(student.level===1){ %>
                        <tbody>
                            <tr>
                                <td>
                                    <div class="row_data" edit_type="click" col_name="fname"><a
                                            href="/students/<%= student._id %>">
                                            <%= student.firstN %>
                                                <%= student.lastN %>
                                        </a></div>
                                </td>
                                <td>
                                    <div class="row_data" edit_type="click" col_name="avg">
                                        <% let engScore=0 %>
                                            <% let engTotal=0 %>
                                                <% for(let subjectAssesment of student.subjectAssesments) { %>
                                                    <% if(subjectAssesment.subject==="English" ){ %>
                                                        <% engScore +=subjectAssesment.studentScore %>
                                                            <% engTotal +=subjectAssesment.totalScore %>
                                                                <% } %>
                                                                    <% } %>
                                                                        <%= Math.floor(engScore/engTotal *100) %>
                                    </div>
                                </td>
                                <td>
                                    <div class="row_data" edit_type="click" col_name="">
                                        <% let mathScore=0 %>
